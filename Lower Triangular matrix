#include <iostream>
using namespace std;

int main()
{
    int n;
    cin >> n;
    int mat[n][n];

    // Input matrix
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < n; j++)
        {
            cin >> mat[i][j];
        }
    }
    bool isLower = true;
    for (int i = 0; i < n; i++)
    {
        for (int j = i + 1; j < n; j++)
        {
            if (mat[i][j] != 0)
            {
                isLower = false;
                break;
            }
        }
    }

    if (isLower)
        cout << "Lower Triangular Matrix" << endl;
    else
        cout << "Not Lower Triangular" << endl;

    return 0;
}
