#include <iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int mat[n][n];
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            cin>>mat[i][j];
        }
    }
    int count=1;
    for(int i=1;i<n;i++)
    {
        for(int j=0;j<i;j++)
        {
            if(mat[i][j]!=0)
            {
                count=0;
                break;
            }
        }
    }
    if(count)
    {
        cout<<"yes";
    }
    else
    {
        cout<<"no";
    }
    return 0;
}
